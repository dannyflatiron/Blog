<h1>New Post!</h1>

<%= render partial: "layouts/errors", locals: {object: @post} %>

<%= form_for(@post) do |f| %>

    <%= f.label :title %>
    <br>
    <%= f.text_field :title %>
    <br>
    <br>
    <%= f.label :content %>
    <br>
    <%= f.text_area :content %>
    <br>
    <%= f.submit %>

<% end %>